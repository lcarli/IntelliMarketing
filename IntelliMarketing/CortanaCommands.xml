<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CortanaCommands_en-us">
    <CommandPrefix> Microsoft </CommandPrefix>
    <Example> How old am I? </Example>

    <Command Name="old">
      <Example> How old am I?  </Example>
      <ListenFor> [would] [you] [please] [tell] [me] How Old [am] [i] [please] </ListenFor>
      <Feedback> Discovering for you right now </Feedback>
      <VoiceCommandService Target="VoiceCommandService"/>
    </Command>
    
    <Command Name="takePhoto">
      <Example> Take Photo </Example>
      <ListenFor> Take Photo </ListenFor>
      <Feedback> Taking </Feedback>
      <Navigate/>
    </Command>

  </CommandSet>

<!-- Other CommandSets for other languages -->

</VoiceCommands>