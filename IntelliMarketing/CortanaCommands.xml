<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CortanaCommands_en-us">
    <CommandPrefix> Microsoft </CommandPrefix>
    <Example> How old am I? </Example>

    <Command Name="old">
      <Example> How old am I?  </Example>
      <ListenFor> [would] [you] [please] [tell] [me] How Old [do] [am] [i] [please] [look] [like] [today] </ListenFor>
      <Feedback> Discovering for you right now </Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="Product">
      <Example> Suggest a product </Example>
      <ListenFor> [would] [you] [please] [suggest] [a] product [for] [me] [please] </ListenFor>
      <Feedback> Let me see who are you first! </Feedback>
      <Navigate/>
    </Command>

